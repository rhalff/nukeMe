{
    "name": "com.rhalff.cordova.nukeme",
    "displayName": "nukeMe",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "start": "node -e \"console.log('open browser at http://localhost:8081/vr/\\n\\n');\" && node node_modules/react-native/local-cli/cli.js start",
        "bundle": "node node_modules/react-vr/scripts/bundle.js",
        "android": "npm run bundle && cordova run android",
        "open": "node -e \"require('xopen')('http://localhost:8081/vr/')\"",
        "devtools": "react-devtools",
        "test": "jest"
    },
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "com.rhalff.sensors": "../cordova-plugin-sensors",
        "cordova-android": "^6.2.3",
        "cordova-plugin-sensors": "file:../cordova-plugin-sensors",
        "cordova-plugin-whitelist": "^1.3.2",
        "gyronorm": "^2.0.6",
        "ovrui": "^1.1.0",
        "react": "~15.4.1",
        "react-native": "~0.42.0",
        "react-vr": "^1.1.0",
        "react-vr-web": "^1.1.0",
        "three": "^0.80.1"
    },
    "devDependencies": {
        "babel-jest": "^19.0.0",
        "babel-preset-react-native": "^1.9.1",
        "jest": "^19.0.2",
        "react-devtools": "^2.1.3",
        "react-test-renderer": "~15.4.1",
        "xopen": "1.0.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "com.rhalff.sensors": {}
        },
        "platforms": [
            "android"
        ]
    },
    "jest": {
        "preset": "react-vr"
    }
}